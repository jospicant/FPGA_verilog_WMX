        {
          "id": "79310625-03c5-4a9f-9063-50c38c13279c",
          "type": "basic.code",
          "data": {
            "code": "reg[7:0] contador, puntero;\nreg[7:0] Canal_1;\nreg PPM, PPM_I;\n\ninitial\n    begin\n    contador = 0;\n    puntero = 0;\n    PPM = 0;\n    PPM_I = 1;\n    Canal_1 = 8;    //8 pulso el canal\n    end\n\nalways @(posedge clk)\n    begin\n    contador <= contador + 1; \n \n    if (contador == 20) // Final de ciclo PPM de 20 mS\n       begin\n       contador <= 0; \n       puntero <= 0 ;\n       end\n   \n    //puntero =0;\n    \n    if (contador == puntero)  // ON 1º impulso\n       begin\n       PPM <= 1 ; PPM_I <= 0; \n       puntero <= puntero + 5;                    // 4 pulsos de duración \n       end\n       \n    //puntero = puntero + 4;\n    \n    if (contador ==  puntero)  // OFF 1º impulso\n       begin\n       PPM <= 0 ; PPM_I <= 1; \n       puntero <= puntero + Canal_1;\n       end\n    \n    //puntero = puntero + Canal_1;\n    \n    if (contador ==  puntero) // ON 2º impulso\n       begin\n       PPM <= 1 ; PPM_I <= 0; \n       puntero <= puntero + 6;\n       end\n\n    //puntero = puntero + 4;                    //4 pulsos de duración\n    \n    if (contador ==  puntero) // OFF 2º impulso\n       begin\n       PPM <= 0 ; PPM_I <= 1; \n        puntero <= puntero + 7;\n       \n       end\n\nend\n\n",
